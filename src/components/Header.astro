---
import Header from '../../../components/Header.astro';

type Props = {
	selected?: string;
};

const properties = Astro.props;
---

<Header
	links={[
		{
			name: 'For Patients',
			href: '/patients',
		},
		{
			name: 'For Nurses',
			href: '/nurses',
		},
		{
			name: 'For Dentists',
			href: '/dentists',
		},
		{
			name: 'Gallery',
			href: '/gallery',
		},
	]}
	selected={properties.selected}
/>
